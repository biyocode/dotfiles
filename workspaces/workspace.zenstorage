#!/bin/bash
kitten @ launch --type=tab --title "zenstorage_server"
sleep 0.2
kitten @ send-text --match "title:^zenstorage_server" "cd ~/00/zenstorage-core\nvenva\n"
sleep 0.1
kitten @ send-text --match "title:^zenstorage_server" "cd ~/00/zenstorage-core\npython main.py\n"

sleep 0.2
kitten @ launch --type=tab --title "zenstorage_code"
sleep 0.2
kitten @ send-text --match "title:^zenstorage_code" "cd ~/00/zenstorage-core\nvenva\n"
